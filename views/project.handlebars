{{main}}

{{"inline "body"}}
  <section class="project">
    <div class="container">
      <h2>{{project.name}}</h2>
      <p>{{project.description}}</p>
      <div class="project-details">
        <p><strong>Created By:</strong> {{project.createdBy}}</p>
        <p><strong>Created At:</strong> {{formatDate project.createdAt}}</p>
      </div>
      <a href="/projects/{{project.id}}/edit" class="btn">Edit Project</a>
    </div>
  </section>
{{/inline}}

<!DOCTYPE html>
<html>
<head>
    <title>Melody Mover</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css">
</head>
<body>
    <header>
        <nav>
            <ul>
                <li><a href="/">Home</a></li>
                <li><a href="/login">Login</a></li>
                <li><a href="/signup">Sign Up</a></li>
                <li><a href="/profile">Profile</a></li>
                <li><a href="/projects">Projects</a></li>
            </ul>
        </nav>
    </header>
    <section class="project">
        <div class="project-content">
            <h1>{{projectName}}</h1>
            <p>{{projectDescription}}</p>
            <!-- Add more project details as needed -->
        </div>
    </section>
    <footer>
        <p>&copy; 2023 Melody Mover. All rights reserved.</p>
    </footer>
</body>
</html>

